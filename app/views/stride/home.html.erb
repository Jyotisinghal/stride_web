<div class="card">
 
    <% if user_signed_in? %>
      <%= link_to "logout", destroy_user_session_path(@user), method: :delete, style: "pull-right" %>
    <% else %>
      <%= link_to "sigup", new_user_session_path %>
      <%= link_to "login", new_user_session_path %>
    <% end %>  
</div>
<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand"><% if @logos.present? %><%= image_tag(@logos.last.image, :width => '70px') %><% else %><%= image_tag("Logo-USB1.png", :width => '70px') %><% end %></a>
    </div>
    <ul class="nav navbar-nav">
      <% Menu.all.each do |menu| %>
        <li><%= menu.name %></li>
      <% end %>
    </ul>
  </div>
</nav>
  